    "clean": "rimraf ./dist",
    "dep-check": "aegir dep-check"
  },
  "devDependencies": {
    "@hyperswarm/testnet": "^1.1.0",
    "@types/debug": "^4.1.7",
    "aegir": "^36.2.3",
    "debug": "^4.3.4",
    "dht-universal": "^0.4.1"
  },
  "eslintConfig": {
    "env": {
      "mocha": true
    },
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "dependencies": {
    "compact-encoding": "^2.6.1",
    "@synonymdev/slashtags-sdk": "~0.1.0"
