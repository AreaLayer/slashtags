{
  "name": "@synonymdev/slashtags-url",
  "version": "1.0.0-alpha.0",
  "description": "Encodes and decodes Slashtags URLs",
  "license": "MIT",
  "homepage": "https://github.com/synonymdev/slashtags/tree/master/packages/url/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synonymdev/slashtags.git"
  },
  "bugs": {
    "url": "https://github.com/synonymdev/slashtags/issues"
  },
  "keywords": [],
  "type": "module",
  "types": "types/src/index.d.ts",
  "typesVersions": {
