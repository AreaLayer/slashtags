{
  "name": "@synonymdev/slashtags-sdk",
  "version": "0.0.0",
  "description": "slashtags development kit",
  "type": "module",
  "keywords": [],
  "homepage": "https://github.com/synonymdev/slashtags/tree/master/packages/sdk/#readme",
  "bugs": {
    "url": "https://github.com/synonymdev/slashtags/issues"
  },
  "license": "MIT",
  "files": [
    "*",
    "!**/*.tsbuildinfo"
  ],
  "types": "types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "types/*",
        "types/src/*"
      ],
      "types/*": [
        "types/*",
        "types/src/*"
      ]
    }
  },
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synonymdev/slashtags.git"
  },
  "scripts": {
    "build": "aegir build",
    "test": "c8 aegir test",
    "lint": "aegir ts -p check && aegir lint --fix",
    "clean": "rimraf ./dist",
    "dep-check": "aegir dep-check"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "aegir": "^36.2.3",
    "debug": "^4.3.4"
  },
  "eslintConfig": {
    "env": {
      "mocha": true
    },
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "dependencies": {
    "b4a": "^1.3.1",
    "blake2b-universal": "^1.0.1",
    "corestore": "^6.0.1-alpha.10",
    "random-access-memory": "^4.0.0",
    "sodium-universal": "^3.1.0"
  }
}
